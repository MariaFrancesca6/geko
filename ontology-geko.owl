<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.example.org/ontology#"
     xml:base="http://www.example.org/ontology"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

    <owl:Ontology/>
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- http://purl.org/emmedi/hico/InterpretationAct -->
    <owl:Class rdf:about="http://purl.org/emmedi/hico/InterpretationAct"/>

    <!-- http://purl.org/emmedi/hico/InterpretationCriterion -->
    <owl:Class rdf:about="http://purl.org/emmedi/hico/InterpretationCriterion"/>

    <!-- http://purl.org/emmedi/hico/InterpretationType -->
    <owl:Class rdf:about="http://purl.org/emmedi/hico/InterpretationType"/>

    <!-- http://iflastandards.info/ns/lrm/lrmoo/F1_Work -->
    <owl:Class rdf:about="http://iflastandards.info/ns/lrm/lrmoo/F1_Work"/>

    <!-- http://iflastandards.info/ns/lrm/lrmoo/F2_Expression -->
    <owl:Class rdf:about="http://iflastandards.info/ns/lrm/lrmoo/F2_Expression"/>

    <!-- http://www.w3.org/ns/oa#Annotation -->
    <owl:Class rdf:about="http://www.w3.org/ns/oa#Annotation">
        <owl:equivalentClass rdf:resource="http://www.w3.org/ns/prov#Entity"/>
    <owl:Class/>

    <!-- http://www.w3.org/ns/oa#SpecificResource -->
    <owl:Class rdf:about="http://www.w3.org/ns/oa#SpecificResource"/>

    <!-- http://www.w3.org/ns/oa#FragmentSelector -->
    <owl:Class rdf:about="http://www.w3.org/ns/oa#FragmentSelector"/>

    <!-- http://www.w3.org/ns/oa#Motivation -->
    <owl:Class rdf:about="http://www.w3.org/ns/oa#Motivation"/>

    <!-- http://www.w3.org/ns/oa#XPathSelector -->
    <owl:Class rdf:about="http://www.w3.org/ns/oa#XPathSelector"/>

    <!-- http://www.w3.org/ns/oa#TextualBody -->
    <owl:Class rdf:about="http://www.w3.org/ns/oa#TextualBody"/>

    <!-- http://www.w3.org/ns/oa#Text -->
    <owl:Class rdf:about="http://www.w3.org/ns/oa#Text"/>

    <!-- http://xmlns.com/foaf/0.1/Person -->
    <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Person"/>

    <!-- http://www.cidoc-crm.org/cidoc-crm/E1_CRM_Entity -->
    <owl:Class rdf:about="http://www.cidoc-crm.org/cidoc-crm/E1_CRM_Entity"/>

    <!-- http://www.cidoc-crm.org/cidoc-crm/E89_Propositional_Object -->
    <owl:Class rdf:about="http://www.cidoc-crm.org/cidoc-crm/E89_Propositional_Object"/>

    <!-- http://iiif.io/api/model/shared-canvas/1.0/#Canvas -->
    <owl:Class rdf:about="http://iiif.io/api/model/shared-canvas/1.0/#Canvas"/>

    <!-- http://iiif.io/api/model/shared-canvas/1.0/#Manifest -->
    <owl:Class rdf:about="http://iiif.io/api/model/shared-canvas/1.0/#Manifest"/>

    <!-- http://www.w3.org/ns/prov#Entity -->
    <owl:Class rdf:about="http://www.w3.org/ns/prov#Entity"/>

    <!-- http://www.w3.org/ns/prov#Collection -->
    <owl:Class rdf:about="http://www.w3.org/ns/prov#Collection"/>

       <!-- http://w3id.org/mlao/Anchor -->
    <owl:Class rdf:about="http://w3id.org/mlao/Anchor"/>

    <!-- https://w3id.org/icon/ontology/PreiconographicalSubject -->
    <owl:Class rdf:about="https://w3id.org/icon/ontology/PreiconographicalSubject"/>

     <!-- https://w3id.org/icon/ontology/IconographicalSubject -->
    <owl:Class rdf:about="https://w3id.org/icon/ontology/IconographicalSubject"/>

     <!-- https://w3id.org/icon/ontology/IconologicalSubject -->
    <owl:Class rdf:about="https://w3id.org/icon/ontology/IconologicalSubject"/>

    <!-- https://w3id.org/geko/Ekphrasis -->
    <owl:Class rdf:about="https://w3id.org/geko/Ekphrasis">
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E89_Propositional_Object"/>
        <owl:equivalentClass rdf:resource="http://www.w3.org/ns/prov#Collection"/>
        <rdfs:comment>An ekphrasis is the textual description of a visual work of art, whether real or imagined. It is composed by two key elements: the textual referent and the iconic referent.</rdfs:comment>
        <rdfs:label>Ekphrasis</rdfs:label>
    </owl:Class>

    <!-- https://w3id.org/geko/Notional -->
    <owl:Class rdf:about="https://w3id.org/geko/Notional">
        <rdfs:comment>A notional (or imagined) ekphrasis describes an artwork that does not exist outside the text itself.</rdfs:comment>
        <rdfs:label>Notional</rdfs:label>
    </owl:Class>

    <!-- https://w3id.org/geko/Mimetic -->
    <owl:Class rdf:about="https://w3id.org/geko/Mimetic">
        <rdfs:comment>A mimetic (or real) ekphrasis describes an actual, physically existing work of art.</rdfs:comment>
        <rdfs:label>Mimetic</rdfs:label>
    </owl:Class>

    <!-- https://w3id.org/geko/Denotation -->
    <owl:Class rdf:about="https://w3id.org/geko/Denotation">
        <rdfs:comment>The denotation modality is the explicit mention of an artwork and its attributes in a text.</rdfs:comment>
        <rdfs:label>Denotation</rdfs:label>
    </owl:Class>

    <!-- https://w3id.org/geko/Dynamization -->
    <owl:Class rdf:about="https://w3id.org/geko/Dynamization">
        <rdfs:comment>The dynamization modality is the dramatization or enactment of the artwork, possibly imagining events unfolding in the described scene.</rdfs:comment>
        <rdfs:label>Dynamization</rdfs:label>
    </owl:Class>
    
    <!-- https://w3id.org/geko/Integration -->
    <owl:Class rdf:about="https://w3id.org/geko/Integration">
        <rdfs:comment>The integration modality is the addition of references to the recipientâ€™s personal and cultural experiences.</rdfs:comment>
        <rdfs:label>Integration</rdfs:label>
    </owl:Class>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- http://purl.org/emmedi/hico/hasInterpretationType -->
    <owl:ObjectProperty rdf:about="http://purl.org/emmedi/hico/hasInterpretationType">
        <rdfs:domain rdf:resource="http://purl.org/emmedi/hico/InterpretationAct"/>
        <rdfs:range rdf:resource="http://purl.org/emmedi/hico/InterpretationType"/>
    </owl:ObjectProperty>

    <!-- http://purl.org/emmedi/hico/hasInterpretationCriterion -->
    <owl:ObjectProperty rdf:about="http://purl.org/emmedi/hico/hasInterpretationCriterion">
        <rdfs:domain rdf:resource="http://purl.org/emmedi/hico/InterpretationAct"/>
        <rdfs:range rdf:resource="http://purl.org/emmedi/hico/InterpretationCriterion"/>
    </owl:ObjectProperty>

    <!-- http://www.w3.org/ns/prov#wasGeneratedBy -->
    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#wasGeneratedBy">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
        <rdfs:range rdf:resource="http://purl.org/emmedi/hico/InterpretationAct"/>
    </owl:ObjectProperty>

    <!-- http://www.w3.org/ns/oa#motivatedBy -->
    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/oa#motivatedBy">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/oa#Motivation"/>
    </owl:ObjectProperty>

    <!-- http://purl.org/dc/terms/creator -->
    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/creator">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    </owl:ObjectProperty>

    <!-- http://www.w3.org/ns/oa#hasTarget -->
    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/oa#hasTarget">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/oa#SpecificResource"/>
    </owl:ObjectProperty>

    <!-- http://www.w3.org/ns/oa#hasBody -->
    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/oa#hasBody">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/oa#TextualBody"/>
    </owl:ObjectProperty>

    <!-- http://www.w3.org/ns/oa#hasSelector -->
    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/oa#hasSelector">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/oa#SpecificResource"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/oa#FragmentSelector"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/oa#XPathSelector"/>
    </owl:ObjectProperty>
    <!-- http://www.w3.org/ns/oa#hasSource -->
    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/oa#hasSource">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/oa#SpecificResource"/>
        <rdfs:range rdf:resource="http://iiif.io/api/model/shared-canvas/1.0/#Canvas"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/oa#Text"/>
    </owl:ObjectProperty>

   <!-- http://w3id.org/mlao/hasAnchor -->
    <owl:ObjectProperty rdf:about="http://w3id.org/mlao/hasAnchor">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
        <rdfs:range rdf:resource="http://w3id.org/mlao/Anchor"/>
    </owl:ObjectProperty>

    <!-- http://w3id.org/mlao/hasConceptualLevel -->
    <owl:ObjectProperty rdf:about="http://w3id.org/mlao/hasConceptualLevel">
        <rdfs:domain rdf:resource="http://w3id.org/mlao/Anchor"/>
        <rdfs:range rdf:resource="https://w3id.org/icon/ontology/PreiconographicalSubject"/>
        <rdfs:range rdf:resource="https://w3id.org/icon/ontology/IconographicalSubject"/>
        <rdfs:range rdf:resource="https://w3id.org/icon/ontology/IconologicalSubject"/>
    </owl:ObjectProperty>

    <!-- http://w3id.org/mlao/isAnchoredTo -->
    <owl:ObjectProperty rdf:about="http://w3id.org/mlao/isAnchoredTo">
        <rdfs:domain rdf:resource="http://w3id.org/mlao/Anchor"/>
        <rdfs:range rdf:resource="http://erlangen-crm.org/current/E1_Entity"/>
    </owl:ObjectProperty>

    <!-- http://purl.org/dc/terms/isPartOf -->
    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/isPartOf">
        <rdfs:domain rdf:resource="http://iiif.io/api/model/shared-canvas/1.0/#Canvas"/>
        <rdfs:range rdf:resource="http://iiif.io/api/model/shared-canvas/1.0/#Manifest"/>
    </owl:ObjectProperty>

    <!-- http://www.w3.org/ns/prov#hadMember -->
    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#hadMember">
        <rdfs:domain rdf:resource="https://w3id.org/geko/Ekphrasis"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
    </owl:ObjectProperty>

    <!-- http://w3id.org/geko/hasEkphrasticModality -->
    <owl:ObjectProperty rdf:about="http://w3id.org/geko/hasEkphrasticModality">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
        <rdfs:range rdf:resource="https://w3id.org/geko/Denotation"/>
        <rdfs:range rdf:resource="https://w3id.org/geko/Dynamization"/>
        <rdfs:range rdf:resource="https://w3id.org/geko/Integration"/>
        <rdfs:comment>The modality links the annotation to the ekphrasis' rhetorical modalities.</rdfs:comment>
        <rdfs:label>has Ekphrastic Modality</rdfs:label>
    </owl:ObjectProperty>

    <!-- http://w3id.org/geko/hasForm -->
    <owl:ObjectProperty rdf:about="http://w3id.org/geko/hasForm">
        <rdfs:domain rdf:resource="https://w3id.org/geko/Ekphrasis"/>
        <rdfs:range rdf:resource="https://w3id.org/geko/Notional"/>
        <rdfs:range rdf:resource="https://w3id.org/geko/Mimetic"/>
        <rdfs:comment>The form assumed by the ekphrastic text, describing a real visual art work or an imagined one.</rdfs:comment>
        <rdfs:label>has Form</rdfs:label>
    </owl:ObjectProperty>

    <!-- http://w3id.org/geko/hasTextualReferent -->
    <owl:ObjectProperty rdf:about="http://w3id.org/geko/hasTextualReferent">
        <rdfs:domain rdf:resource="https://w3id.org/geko/Ekphrasis"/>
        <rdfs:range rdf:resource="http://iflastandards.info/ns/lrm/lrmoo/F2_Expression"/>
        <rdfs:comment>The textual component of the ekphrasis.</rdfs:comment>
        <rdfs:label>has Textual Referent</rdfs:label>
    </owl:ObjectProperty>

    <!-- http://w3id.org/geko/hasIconicReferent -->
    <owl:ObjectProperty rdf:about="http://w3id.org/geko/hasIconicReferent">
        <rdfs:domain rdf:resource="https://w3id.org/geko/Ekphrasis"/>
        <rdfs:range rdf:resource="http://iflastandards.info/ns/lrm/lrmoo/F1_Work"/>
        <rdfs:comment>The iconic component of the ekphrasis.</rdfs:comment>
        <rdfs:label>has Iconic Referent</rdfs:label>
    </owl:ObjectProperty>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- http://www.w3.org/ns/prov#statedAtTime -->
    <owl:DataProperty rdf:about="http://www.w3.org/ns/prov#statedAtTime">
        <rdfs:domain rdf:resource="http://purl.org/emmedi/hico/InterpretationAct"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DataProperty>

    <!-- http://purl.org/dc/terms/created -->
    <owl:DataProperty rdf:about="http://purl.org/dc/terms/created">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DataProperty>

    <!-- http://xmlns.com/foaf/0.1/name -->
    <owl:DataProperty rdf:about="http://xmlns.com/foaf/0.1/name">
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DataProperty>

    <!-- http://www.w3.org/1999/02/22-rdf-syntax-ns#value -->
    <owl:DataProperty rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#value">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/oa#TextualBody"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/oa#FragmentSelector"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/oa#XPathSelector"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DataProperty>

</rdf:RDF>
